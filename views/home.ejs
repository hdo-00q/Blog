<%- include("partials/header") -%>
<h1><%= header %></h1>

<p><%= text %></p>

<% posts.forEach(function(post) { %>
  <div>
    <h1><%= post.journalTitle %></h1>
    <p>
      <%= post.journalEntry %>
      <a href=<%= post.journalLink %>>Read More</a>
    </p>
  </div>
<% }) %>
<%- include("partials/footer") -%>
